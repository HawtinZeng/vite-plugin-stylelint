{
  "name": "vite-plugin-stylelint",
  "version": "1.0.3",
  "description": "Stylelint plugin for vite.",
  "keywords": [
    "stylelint",
    "vite-plugin"
  ],
  "homepage": "https://github.com/ModyQyW/vite-plugin-stylelint",
  "repository": "git+https://github.com:ModyQyW/vite-plugin-stylelint.git",
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui7@mail3.sysu.edu.cn"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "npm run build -- --watch src",
    "example:react-build": "pnpm run build --filter example-react",
    "example:react-dev": "pnpm run dev --filter example-react",
    "example:vue-build": "pnpm run build --filter example-vue",
    "example:vue-dev": "pnpm run dev --filter example-vue",
    "lint": "eslint src",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@rollup/pluginutils": "^4.1.1",
    "@types/eslint": "^7.28.2",
    "@types/node": "^16.11.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.0.0",
    "tsup": "^4.14.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.10"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin main && npm publish"
    }
  }
}
